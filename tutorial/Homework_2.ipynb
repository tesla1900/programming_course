    "# defining the second function in eqn. (3)\n",
    "def backward(a, n):\n",
    "    if n == 50: return 0\n",
    "    a = (1 / 10) * ((1 / (n+1))  - a)\n",
    "    # we take n + 1 above because for each case, our loop is 1 number behind the desired value\n",
    "    return a\n",
    "\n",
    "i = 0 # declaring the value for I_50\n",
    "\n",
    "# computing the value for I_20\n",
    "for n in range(50, 19, -1):\n",
    "    i = backward(i, n)\n",
    "    print('I_%d =' % n, i)"
   ]
