    "# defining the first function in eqn. (2)\n",
    "def forward(a, n):\n",
    "    if n == 0: return np.log(11/10)\n",
    "    a = (1 / n) - (a * 10)\n",
    "    return a\n",
    "    \n",
    "i = np.log(11/10) #declaring the value of I_0\n",
    "\n",
    "# computing the value of I_20\n",
    "for n in range(0, 21):\n",
    "    i = forward(i, n)\n",
    "    print ('I_%d =' % n, i)"
